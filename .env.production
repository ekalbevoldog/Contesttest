# Production environment variables
NODE_ENV=production

# Database configuration will be provided by environment variables
# during deployment or in the platform's environment settings

# Web server configuration
PORT=5000
HOST=0.0.0.0

# Set error logging level (silent, error, warn, info, verbose, debug)
LOG_LEVEL=error

# Disable development features
VITE_DEV_TOOLS=false

# Performance settings
MAX_DB_CONNECTIONS=20
DB_CONNECTION_TIMEOUT=5000